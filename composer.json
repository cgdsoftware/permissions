{
    "name": "cgdsoftware/permissions",
    "description": "Permissions dependency for Laravel Enso",
    "keywords": [
        "cgdsoftware",
        "permission-manager",
        "laravel-permissions"
    ],
    "homepage": "https://github.com/cgdsoftware/permissions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://cgdsoftware.com",
            "role": "Developer"
        }
    ],
    "require": {
        "cgdsoftware/core": "^8.0",
        "cgdsoftware/dynamic-methods": "^2.0",
        "cgdsoftware/enums": "^2.0",
        "cgdsoftware/forms": "^4.0",
        "cgdsoftware/helpers": "^2.0",
        "cgdsoftware/migrator": "^2.0",
        "cgdsoftware/roles": "^5.0",
        "cgdsoftware/tables": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Permissions\\": "src/",
            "LaravelEnso\\Permissions\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Permissions\\AppServiceProvider",
                "LaravelEnso\\Permissions\\AuthServiceProvider",
                "LaravelEnso\\Permissions\\EnumServiceProvider"
            ],
            "aliases": {}
        }
    }
}